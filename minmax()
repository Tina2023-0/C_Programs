#include<stdio.h>
void minmax(int a,int b,int c,int *min,int *max)
{

*min=a<b&&a<c?a:b<c?b:c ;
*max=a>b&&a>c?a:b>c?b:c;
}
int main()
{
int a,b,c,min,max;
printf("input the three numbers");
scanf("%d%d%d",&a,&b,&c);
minmax(a,b,c,&min,&max);
printf("\n minimum=%d",min);
printf("\n maximum=%d",max);
return 0;
}
    
