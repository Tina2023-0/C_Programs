using namespace std;
#include<iostream>
inline int add(int a,int b)
{
    return a+b;
}
int square(int a)
{
    cout<<"\n square of two given numbers"<<a*a;
}
int main()
{
    int a,b;
    cout<<"\n Enter first number :";
    cin>>a;
    cout<<"\n Enter second number :";
    cin>>b;
    cout<<"\n Addition of two numbers "<<add(a,b);
    square(a);
    return 0;
}
